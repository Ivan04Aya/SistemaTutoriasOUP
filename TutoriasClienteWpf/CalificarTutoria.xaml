<Window x:Class="TutoriasClienteWpf.CalificarTutoria"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TutoriasClienteWpf"
        mc:Ignorable="d"
        Title="CalificarTutoria" Height="404" Width="496">
    <Grid>
        <Label Content="Calificar Tutoria" HorizontalAlignment="Left" Margin="63,36,0,0" VerticalAlignment="Top" FontSize="36" FontWeight="Bold"/>
        <Label Content="Elige la calificacion que creas pertinente:" HorizontalAlignment="Left" Margin="63,125,0,0" VerticalAlignment="Top" FontSize="20"/>
        <ComboBox x:Name="cbPuntaje" HorizontalAlignment="Left" Margin="63,194,0,0" VerticalAlignment="Top" Width="384" Height="50" SelectedIndex="3" FontSize="28">
            <ComboBoxItem Content="1"/>
            <ComboBoxItem Content="2"/>
            <ComboBoxItem Content="3"/>
            <ComboBoxItem Content="4"/>
            <ComboBoxItem Content="5"/>
        </ComboBox>
        <Button Content="Calificar" HorizontalAlignment="Center" Margin="0,287,0,0" VerticalAlignment="Top" FontSize="20" Height="55" Width="140" Background="#75D846" Click="BtnCalificar"/>

    </Grid>
</Window>
